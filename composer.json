{
    "name": "wexample/php-yaml",
    "description": "A lightweight PHP library for resolving includes between YAML files",
    "type": "library",
    "license": "MIT",
    "version": "1.0.18",
    "autoload": {
        "psr-4": {
            "Wexample\\PhpYaml\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Wexample\\PhpYaml\\Tests\\": "tests/"
        }
    },
    "require": {
        "php": ">=8.1",
        "symfony/yaml": "^6.0|^7.0",
        "wexample/php-helpers": "^1.0.45"
    },
    "require-dev": {
        "phpunit/phpunit": "^9.5"
    }
}
