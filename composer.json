{
    "name": "wexample/php-yaml-includes",
    "description": "A lightweight PHP library for resolving includes between YAML files",
    "type": "library",
    "license": "MIT",
    "autoload": {
        "psr-4": {
            "Wexample\\PhpYamlIncludes\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Wexample\\PhpYamlIncludes\\Tests\\": "tests/"
        }
    },
    "require": {
        "php": ">=8.1",
        "symfony/yaml": "^6.0|^7.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^9.5"
    }
}
